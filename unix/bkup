#!/bin/sh

. ${DIR}data/config
destination="${WAREDIR}"

target_file="$1"
if [ ! -e "$target_file" ]; then
  echo 'aborted.'
  exit 255
fi

timestamp=`date +%Y%m%d%H%M%S`
destination="${destination}$(basename $target_file)_${timestamp}"

mv "$target_file" "${destination} && echo "moved: ${destination}"